{
  "name": "llp-script",
  "version": "0.1.2",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^24.0.19",
    "browserify": "^16.5.0",
    "jest": "^24.9.0",
    "jison": "^0.4.18",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.4"
  },
  "publishConfig": {},
  "main": "dist/expression.js",
  "scripts": {
    "test": "jest",
    "jison": "jison src/parser.y -o src/parser.js -m js&&echo 'module.exports=parser;' >> src/parser.js",
